<config>
  <vrfs xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-infra-rsi-cfg">
   <vrf>
    <vrf-name>JIE</vrf-name>
    <create/>
    <afs>
     <af>
      <af-name>ipv4</af-name>
      <saf-name>unicast</saf-name>
      <topology-name>default</topology-name>
      <create/>
      <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-bgp-cfg">
       <import-route-targets>
        <route-targets>
         <route-target>
          <type>as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65001</as>
           <as-index>65400</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
        </route-targets>
       </import-route-targets>
       <export-route-targets>
        <route-targets>
         <route-target>
          <type>as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65001</as>
           <as-index>65400</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
        </route-targets>
       </export-route-targets>
      </bgp>
     </af>
    </afs>
   </vrf>
   <vrf>
    <vrf-name>ARMY</vrf-name>
    <create/>
    <afs>
     <af>
      <af-name>ipv4</af-name>
      <saf-name>unicast</saf-name>
      <topology-name>default</topology-name>
      <create/>
      <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-bgp-cfg">
       <import-route-targets>
        <route-targets>
         <route-target>
          <type>as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65001</as>
           <as-index>65400</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
         <route-target>
          <type>four-byte-as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65555</as>
           <as-index>1</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
        </route-targets>
       </import-route-targets>
       <export-route-targets>
        <route-targets>
         <route-target>
          <type>as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65001</as>
           <as-index>65400</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
         <route-target>
          <type>four-byte-as</type>
          <as-or-four-byte-as>
           <as-xx>0</as-xx>
           <as>65555</as>
           <as-index>0</as-index>
           <stitching-rt>0</stitching-rt>
          </as-or-four-byte-as>
         </route-target>
        </route-targets>
       </export-route-targets>
      </bgp>
     </af>
    </afs>
   </vrf>
  </vrfs>
</config
