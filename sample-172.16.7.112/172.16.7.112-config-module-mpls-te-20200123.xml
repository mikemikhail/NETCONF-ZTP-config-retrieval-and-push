<config>
  <mpls-te xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-mpls-te-cfg">
   <enable-traffic-engineering/>
   <interfaces>
    <interface>
     <interface-name>GigabitEthernet0/0/0/0.1112</interface-name>
     <lcac>
      <enable/>
     </lcac>
     <global-attributes>
      <auto-tunnel>
       <backup>
        <enable/>
        <attribute-set>BU</attribute-set>
       </backup>
      </auto-tunnel>
     </global-attributes>
    </interface>
    <interface>
     <interface-name>GigabitEthernet0/0/0/0.1212</interface-name>
     <lcac>
      <enable/>
     </lcac>
     <global-attributes>
      <auto-tunnel>
       <backup>
        <enable/>
        <attribute-set>BU</attribute-set>
       </backup>
      </auto-tunnel>
     </global-attributes>
    </interface>
   </interfaces>
   <global-attributes>
    <pce-attributes>
     <peers>
      <peer>
       <pce-peer-address>192.168.30.101</pce-peer-address>
       <enable/>
      </peer>
      <peer>
       <pce-peer-address>192.168.30.102</pce-peer-address>
       <enable/>
      </peer>
     </peers>
     <pce-stateful>
      <instantiation/>
      <enable/>
     </pce-stateful>
    </pce-attributes>
    <log-preemption/>
    <log-frr-protection>all</log-frr-protection>
    <auto-tunnel>
     <pcc>
      <tunnel-range>
       <min-tunnel-id>51200</min-tunnel-id>
       <max-tunnel-id>51299</max-tunnel-id>
      </tunnel-range>
     </pcc>
     <mesh>
      <mesh-groups>
       <mesh-group>
        <mesh-group-id>0</mesh-group-id>
        <create/>
        <attribute-set>TE</attribute-set>
        <destination-list>TE</destination-list>
       </mesh-group>
      </mesh-groups>
      <tunnel-range>
       <min-tunnel-id>11200</min-tunnel-id>
       <max-tunnel-id>11249</max-tunnel-id>
      </tunnel-range>
     </mesh>
     <backup>
      <tunnel-range>
       <min-tunnel-id>11250</min-tunnel-id>
       <max-tunnel-id>11299</max-tunnel-id>
      </tunnel-range>
     </backup>
    </auto-tunnel>
    <advertise-explicit-nulls/>
    <attribute-set>
     <auto-backup-attributes>
      <auto-backup-attribute>
       <attribute-set-name>BU</attribute-set-name>
       <enable/>
       <priority>
        <setup-priority>6</setup-priority>
        <hold-priority>6</hold-priority>
       </priority>
       <record-route/>
      </auto-backup-attribute>
     </auto-backup-attributes>
     <auto-mesh-attributes>
      <auto-mesh-attribute>
       <attribute-set-name>TE</attribute-set-name>
       <enable/>
       <interface-bandwidth>1000000</interface-bandwidth>
       <priority>
        <setup-priority>3</setup-priority>
        <hold-priority>3</hold-priority>
       </priority>
       <bandwidth>
        <dste-type>standard-dste</dste-type>
        <class-or-pool-type>0</class-or-pool-type>
        <bandwidth>1000000</bandwidth>
       </bandwidth>
       <autoroute-announce/>
       <fast-reroute>
        <bandwidth-protection>1</bandwidth-protection>
        <node-protection>1</node-protection>
       </fast-reroute>
       <record-route/>
      </auto-mesh-attribute>
     </auto-mesh-attributes>
    </attribute-set>
    <path-selection>
     <ignore-overload-role>
      <head>true</head>
      <tail>true</tail>
     </ignore-overload-role>
    </path-selection>
    <reoptimize-delay-install-timer>0</reoptimize-delay-install-timer>
   </global-attributes>
  </mpls-te>
</config
