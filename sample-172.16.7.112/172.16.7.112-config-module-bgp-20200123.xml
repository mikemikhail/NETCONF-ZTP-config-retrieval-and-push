<config>
  <bgp xmlns="http://cisco.com/ns/yang/Cisco-IOS-XR-ipv4-bgp-cfg">
   <instance>
    <instance-name>default</instance-name>
    <instance-as>
     <as>0</as>
     <four-byte-as>
      <as>65001</as>
      <bgp-running/>
      <default-vrf>
       <global>
        <router-id>10.101.112.1</router-id>
        <global-afs>
         <global-af>
          <af-name>ipv4-unicast</af-name>
          <enable/>
          <static-routes>
           <route-policy-name>RR_TEST</route-policy-name>
          </static-routes>
          <isis-routes>
           <isis-route>
            <instance-name>ISIS</instance-name>
            <route-policy-name>LB-O</route-policy-name>
           </isis-route>
          </isis-routes>
          <allocate-label>
           <all>true</all>
          </allocate-label>
         </global-af>
         <global-af>
          <af-name>vpnv4-unicast</af-name>
          <enable/>
         </global-af>
         <global-af>
          <af-name>ipv6-unicast</af-name>
          <enable/>
         </global-af>
         <global-af>
          <af-name>l2vpn-vpls</af-name>
          <enable/>
         </global-af>
         <global-af>
          <af-name>lsls</af-name>
          <enable/>
          <domain-distinguisher>
           <as>65001</as>
           <router-id>10.101.112.1</router-id>
          </domain-distinguisher>
         </global-af>
        </global-afs>
       </global>
       <bgp-entity>
        <neighbor-groups>
         <neighbor-group>
          <neighbor-group-name>RR-EVEN</neighbor-group-name>
          <create/>
          <remote-as>
           <as-xx>0</as-xx>
           <as-yy>65001</as-yy>
          </remote-as>
          <keychain>
           <keychain-disable>false</keychain-disable>
           <keychain-name>CISCO</keychain-name>
          </keychain>
          <update-source-interface>Loopback0</update-source-interface>
          <neighbor-group-afs>
           <neighbor-group-af>
            <af-name>ipv4-labeled-unicast</af-name>
            <activate/>
            <route-policy-out>RR-O</route-policy-out>
            <next-hop-self>true</next-hop-self>
           </neighbor-group-af>
           <neighbor-group-af>
            <af-name>vpnv4-unicast</af-name>
            <activate/>
           </neighbor-group-af>
           <neighbor-group-af>
            <af-name>ipv6-unicast</af-name>
            <activate/>
           </neighbor-group-af>
           <neighbor-group-af>
            <af-name>l2vpn-vpls</af-name>
            <activate/>
           </neighbor-group-af>
          </neighbor-group-afs>
         </neighbor-group>
        </neighbor-groups>
        <neighbors>
         <neighbor>
          <neighbor-address>10.21.12.2</neighbor-address>
          <remote-as>
           <as-xx>0</as-xx>
           <as-yy>65002</as-yy>
          </remote-as>
          <keychain>
           <keychain-disable>false</keychain-disable>
           <keychain-name>CISCO</keychain-name>
          </keychain>
          <ebgp-multihop>
           <max-hop-count>255</max-hop-count>
           <mpls-deactivation>false</mpls-deactivation>
          </ebgp-multihop>
          <description>ASBR 65002 </description>
          <neighbor-afs>
           <neighbor-af>
            <af-name>ipv4-labeled-unicast</af-name>
            <activate/>
            <route-policy-in>LB-I</route-policy-in>
            <route-policy-out>LB-O</route-policy-out>
           </neighbor-af>
          </neighbor-afs>
         </neighbor>
         <neighbor>
          <neighbor-address>10.100.102.1</neighbor-address>
          <neighbor-group-add-member>RR-EVEN</neighbor-group-add-member>
         </neighbor>
         <neighbor>
          <neighbor-address>10.100.104.1</neighbor-address>
          <neighbor-group-add-member>RR-EVEN</neighbor-group-add-member>
         </neighbor>
        </neighbors>
       </bgp-entity>
      </default-vrf>
      <vrfs>
       <vrf>
        <vrf-name>JIE</vrf-name>
        <vrf-global>
         <exists/>
         <route-distinguisher>
          <type>as</type>
          <as-xx>0</as-xx>
          <as>65001</as>
          <as-index>65400</as-index>
         </route-distinguisher>
         <vrf-global-afs>
          <vrf-global-af>
           <af-name>ipv4-unicast</af-name>
           <enable/>
           <connected-routes/>
           <allocate-label>
            <all>true</all>
           </allocate-label>
          </vrf-global-af>
         </vrf-global-afs>
        </vrf-global>
        <vrf-neighbors>
         <vrf-neighbor>
          <neighbor-address>10.112.1.4</neighbor-address>
          <remote-as>
           <as-xx>0</as-xx>
           <as-yy>65400</as-yy>
          </remote-as>
          <vrf-neighbor-afs>
           <vrf-neighbor-af>
            <af-name>ipv4-labeled-unicast</af-name>
            <activate/>
            <route-policy-in>ALL</route-policy-in>
            <route-policy-out>ALL</route-policy-out>
            <as-override>true</as-override>
           </vrf-neighbor-af>
          </vrf-neighbor-afs>
         </vrf-neighbor>
        </vrf-neighbors>
       </vrf>
      </vrfs>
     </four-byte-as>
    </instance-as>
   </instance>
  </bgp>
</config
